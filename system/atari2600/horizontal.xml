<!-- Atari 2600 -->

<theme>
	<formatVersion>7</formatVersion>

	<view name="system">
		<image name="frog" extra="static">
			<origin>0 0</origin>
			<pos>0.4 -0.1</pos>
			<size>0.29375 0.9092593</size>									
			<scale>0</scale>
			<rotation>0</rotation>
			<zIndex>19</zIndex>
			<opacity>1</opacity>
			<path>./frog.png</path>

			<storyboard repeat="forever">
				
				<animation property="y" to="0" begin="" duration="800" mode="bump"/> 
				<animation property="scale" from="0" to="0.7" begin="" duration="4800" mode="bump"/>
				<animation property="x" to="0.17" begin="1900" duration="500" mode="EaseOut"/> 
				<animation property="x" to="0" begin="4800" duration="200" mode="EaseIn"/> 
				<animation property="x" to="0.16" begin="5900" duration="500 " mode="EaseIn"/> 
				
				<animation property="x" to="0.2" begin="7200" duration="500" mode="EaseIn"/> 
				<animation property="y" to="-0.1" begin="7700" duration="250" mode="EaseIn"/> 
				<animation property="x" to="0.25" begin="7700" duration="250" mode="EaseIn"/> 
				<animation property="x " to="0.3" begin="7950" duration="250" mode="EaseIn"/> 
				<animation property="y" to="0.1" begin="8500" duration="250" mode="EaseIn"/> 
				<animation property="x" to="0.15" begin="8500" duration="250" mode="EaseIn"/> 
				
				<animation property="y" to="0.08" begin="8900" duration="1000" mode="bump"/> 
				<animation property="x" to="0.44" begin="8900" duration="1000" mode="EaseOut"/> 
				<animation property="scale" to="0.85" begin="8900" duration="1000" mode="linear"/>
				
				<animation property="y" to="-1" begin="16000" duration="1000" mode="EaseOut"/> 
				<animation property="x" to="-1" begin="16000" duration="1000" mode="EaseIn"/> 
				
			</storyboard>

		
		</image>
		
		
		<image name="wink" extra="static">
			<origin>0 0</origin>
			<pos>0.484 0.223</pos>
			<size>0.0291667 0.0481481</size>									

			<scale>0.85</scale>
			<rotation>0</rotation>
			<zIndex>20</zIndex>
			<opacity>0</opacity>
			<path>./wink.png</path>


			<storyboard repeat="forever">
				<animation property="opacity" from="0" to="1" begin="12000" duration="300" mode="linear"/>
				<animation property="opacity" from="1" to="0" begin="12300" duration="300" mode="linear"/>
				<animation property="opacity" from="0" to="1" begin="14000" duration="300" mode="linear"/>
				<animation property="opacity" from="1" to="0" begin="14300" duration="300" mode="linear"/>
				<animation property="opacity" to="0" begin="16000" duration="1000" mode="linear"/>
			</storyboard>

		
		</image>
		
		
		
		
	</view>

</theme>
