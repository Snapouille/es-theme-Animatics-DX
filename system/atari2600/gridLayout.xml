<!-- Atari 2600 -->

<theme>
	<formatVersion>7</formatVersion>
	

	<view name="system">
	
	
		<image name="BG_snapLegacy" extra="true">
			<size>1 1</size>									
			<pos>0.5 0.44</pos>	<scale>1</scale>	
			<pos ifHelpPrompts="false">0.5 0.5</pos>	
			<saturation>0.7</saturation>
			  <storyboard>
				<animation property="saturation" to="1" begin="" duration="500" mode="linear"/>
			  </storyboard>
		</image>
		
		
	
		<image name="frog" extra="static">
			<origin>0.5 0.5</origin>
			<size>0.29375 0.9092593</size>									
			<pos>1.55 1.55</pos>
			<scale>1</scale>
			<path>./frog.png</path>
			

			<storyboard>
				<animation property="offsetY" to="-1" begin="100" duration="600" mode="Bump"/>
				<animation property="offsetX" to="-0.71" begin="100" duration="600" mode="EaseInOut"/>
			</storyboard>

		
		</image>
		
		
		<image name="wink" extra="static">
			<origin>0.5 0.5</origin>
			<pos>0.76 0.286</pos>
			<size>0.0291667 0.0481481</size>									

			<scale>1</scale>
			<rotation>0</rotation>
			<zIndex>9901</zIndex>
			<opacity>0</opacity>
			<path>./wink.png</path>

			<storyboard repeat="forever">
				<animation property="opacity" from="0" to="1" begin="2000" duration="100" mode="linear"/>
				<animation property="opacity" from="1" to="0" begin="2300" duration="300" mode="linear"/>
				<animation property="opacity" from="0" to="1" begin="4000" duration="100" mode="linear"/>
				<animation property="opacity" from="1" to="0" begin="4300" duration="300" mode="linear"/>
				<animation property="opacity" to="0" begin="10000" duration="10" mode="linear"/>
			</storyboard>
			
			
		</image>
		
		
		
		
	</view>

</theme>
