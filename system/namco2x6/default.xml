<!-- Namco 2x6 -->

<theme>
	<formatVersion>7</formatVersion>
	
	<view name="system">

		<image name="avatar" extra="static">
			<origin>0.5 0.5</origin>	
			<size>0.3625 0.8333333</size>									
			<pos>0.41 0.47</pos>	
			<scale>0.55</scale>
			<linearSmooth>true</linearSmooth>					
			<storyboard>
				
        		<animation property="x" to="0.51" begin="" duration="800" mode="EaseInOut"/>
				<animation property="scale" to="0.9" begin="" duration="800" mode="EaseInOut"/>
        		<animation property="y" to="0.41" begin="" duration="800" mode="Bump"/>
				
			</storyboard>					

		</image>
		
	</view>
</theme>
