<!-- vitaQuake -->

<theme>
	<formatVersion>7</formatVersion>
	
	<view name="system">
	
		<image name="BG_snapLegacy" extra="true">
			<flipX>true</flipX>
		</image>

		<image name="avatar" extra="static">
			<origin>0.5 0.5</origin>	
			<size>0.3802083 0.8509259</size>									
			<pos>0.80 0.51</pos>	
			<scale>0.7</scale>
			<linearSmooth>true</linearSmooth>	
			
			<storyboard>
				
        		<animation property="x" to="0.82" begin="" duration="1000" mode="EaseInOut"/>
				<animation property="scale" to="0.85" begin="" duration="1000" mode="EaseInOut"/>
        		<animation property="y" to="0.54" begin="" duration="1200" mode="EaseInOut"/>
				
			</storyboard>					

		</image>
		
	</view>
</theme>
