<!-- X68000 -->


<theme>
	<formatVersion>7</formatVersion>

	<view name="system">
	
		
	
	
	
		<image name="avatar-girl" extra="static">
			<origin>0 0</origin>									
			<size>0.3171875 0.7407407</size>									
			<pos>0.11 0.23</pos>
			<scale>0.6</scale>
			<opacity>1</opacity>
			<path>./avatar-girl.png</path>
			<zIndex>20</zIndex>

			<storyboard repeat="forever">
					
					<animation property="x" to="0.17" begin="500" duration="1500" mode="EaseOut"/>
					
					<animation property="scale" to="1" begin="2000" duration="350" mode="Linear"/>
					<animation property="x" to="0.33" begin="2000" duration="375" mode="EaseOut"/>
					<animation property="y" to="0.05" begin="2000" duration="375" mode="EaseOut"/>

					
					
					<animation property="x" to="1" begin="25000" duration="500" mode="linear"/>
					<animation property="opacity" to="0" begin="25500" duration="500" mode="linear"/>
					
			</storyboard>
		
		
		</image>
		
		
		
		
		
		
		
		
		
		
	</view>

</theme>
