<!-- TMNT collection -->

<theme>
	<formatVersion>7</formatVersion>
	
	<view name="system">

		<image name="avatar" extra="static">
			<origin>0.5 0.5</origin>	
			<size>0.375 0.7592593</size>									
			<pos>0.88 -1</pos>	
			<scale>0.1</scale>
			<linearSmooth>true</linearSmooth>					
			<storyboard>
				
				<animation property="scale" to="1" begin="" duration="3000" mode="EaseOutQuint"/>
        		<animation property="x" to="0.82" begin="" duration="3000" mode="EaseOutQuint"/>
        		<animation property="y" to="0.55" begin="" duration="3000" mode="EaseOutQuint"/>
        		<animation property="rotation" to="-10" begin="" duration="3000" mode="EaseOutQuint"/>
				
			</storyboard>					

		</image>
		
	</view>
</theme>
