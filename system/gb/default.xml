		<storyboard repeat="forever">
				<animation property="x" to="-1" begin="14000" duration="2000" mode="linear"/>
				<animation property="x" from="-1" to="-1" begin="16000" duration="4000" mode="linear"/>
		</storyboard>
		</image>
	
	
	
	
	
	<image name="tuyau" extra="true">	
		<storyboard>
				<animation property="opacity" from="0" to="1" begin="90" duration="10" mode="linear"/>
		</storyboard>
	</image>
		
	<image name="mario0" extra="true">	
		<storyboard repeat="forever">
				<animation property="opacity" from="0" to="1" begin="" duration="1000" mode="linear"/>
				<animation property="y" to="0.468" begin="100" duration="2000" mode="linear"/>
				<animation property="opacity" from="1" to="0" begin="1800" duration="1000" mode="linear"/>
				<animation property="opacity" from="0" to="0" begin="3000" duration="17000" mode="linear"/>
		</storyboard>
	</image>
	
	<image name="mario1" extra="true">	
		<storyboard repeat="forever">
				<animation property="opacity" from="0" to="1" begin="2000" duration="1000" mode="linear"/>
				<animation property="x" to="0.035" begin="3000" duration="1000" mode="linear"/>
				<animation property="y" to="0.05" begin="3000" duration="1000" mode="bump"/>
				<animation property="scale" to="1" begin="3000" duration="1000" mode="EaseOut"/>
				<!-- <animation property="scale" to="1" begin="4000" duration="6000" mode="linear"/> -->
        		<animation property="opacity" begin="3500" from="1" to="0" duration="500" mode="linear"/>
				<animation property="opacity" from="0" to="0" begin="5000" duration="15000" mode="linear"/>
		</storyboard>
	</image>
	
	<image name="mario2" extra="true">	
		<storyboard repeat="forever">
				<animation property="x" to="0.035" begin="3000" duration="1000" mode="linear"/>
				<animation property="y" to="0.05" begin="3000" duration="1000" mode="bump"/>
				<animation property="scale" to="1" begin="3000" duration="1000" mode="EaseOut"/>
				<animation property="opacity" from="0" to="1" begin="3500" duration="500" mode="linear"/>
				
				<animation property="y" to="0.035" begin="4000" duration="12800" mode="EaseIn"/>
				
								
				<animation property="rotation" to="720" begin="14800" duration="2000" mode="EaseOut"/>
				<animation property="y" to="-1" begin="15000" duration="800" />
				<animation property="x" to="-1" begin="15000" duration="1500" />
				<animation property="opacity" from="1" to="0" begin="14500" duration="5500" mode="linear"/>
		</storyboard>
	</image>
	
	
	<image name="gombaG" extra="true">	
		<storyboard repeat="forever">
				<animation property="opacity" from="0" to="1" begin="" duration="100" mode="linear"/>
				<animation property="x" to="0.32" begin="" duration="6000" mode="linear"/>
				<animation property="opacity" from="1" to="0" begin="6000" duration="100" mode="linear"/>
				<animation property="x" to="0.55" begin="6000" duration="10100" mode="linear"/>
		</storyboard>
	</image>
		
	<image name="gombaD" extra="true">	
		<storyboard repeat="forever">
				<animation property="opacity" from="0" to="1" begin="6000" duration="100" mode="linear"/>
				<animation property="x" to="0.55" begin="6000" duration="6000" mode="linear"/>
				<animation property="opacity" from="1" to="0" begin="12000" duration="100" mode="linear"/>
				<animation property="x" to="0.32" begin="12100" duration="4000" mode="linear"/>
		</storyboard>
	</image>
		
		
		
		
