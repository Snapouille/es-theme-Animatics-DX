<!-- ImageViewer -->

<theme>
	<formatVersion>7</formatVersion>

	<view name="system">
	
	
	
		<image name="BG_snapLegacy" extra="true">
			<size>1 1</size>									
			<pos>0.5 0.44</pos>	
			<scale>1</scale>	
			<pos ifHelpPrompts="false">0.5 0.5</pos>
			<flipX>true</flipX>
			<saturation>0.7</saturation>
			  <storyboard>
				<animation property="saturation" to="1" begin="" duration="500" mode="linear"/>
			  </storyboard>
		</image>
		
	
		<image name="girlbeyond" extra="static">
			<origin>0.5 0.5</origin>
			<size>0.4390625 0.7407407</size>									
			<pos>1.05 0.661</pos>									
			<path>./girlbeyond.png</path>
			<scale>0.75</scale>	
			<storyboard>
				<animation property="offsetX" to="-0.218" begin="100" duration="500" mode="EaseOut"/>
			</storyboard>
			
			<storyboard ifHelpPrompts="false">
				<animation property="offsetY"  to="0.06"/>
				<animation property="offsetX" to="-0.218" begin="100" duration="500" mode="EaseOut"/>
			</storyboard>
		
		</image>
	
	
	
	
	</view>

</theme>
