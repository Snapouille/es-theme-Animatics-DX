<!-- GB 2 Players -->

<theme>
	<formatVersion>7</formatVersion>

	<view name="system">
	
		<image name="avatar-run, avatar-run-flip, avatar-stand, avatar-surprised">
			<origin>0 0</origin>
			<scale>0.5</scale>
			<opacity>0</opacity>
			<zIndex>11</zIndex>
			<linearSmooth>true</linearSmooth>
		</image>	
	
		<image name="avatar-run" extra="true">
			<pos>0.7 0.39</pos>
			<size>0.1192708 0.2842593</size>									
			<path>./avatar-run.gif</path>
			<rotation>8</rotation>
			
			<storyboard repeat="forever">
				<animation property="opacity" to="1" begin="" duration="10" mode="linear"/>
				<animation property="x" to="0.04" begin="" duration="2000" mode="linear"/>
				<animation property="opacity" to="0" begin="2000" duration="10" mode="linear"/>
				<animation property="x" to="0.12" begin="2000" duration="400" mode="linear"/>
				<animation property="opacity" to="1" begin="5700" duration="10" mode="linear"/>
				<animation property="x" to="-0.1" begin="5700" duration="600" mode="linear"/>
				
				<animation property="opacity" to="0" begin="16000" duration="10" mode="linear"/>
			</storyboard>
		</image>	

		<image name="avatar-run-flip" extra="true">
			<pos>0.04 0.39</pos>
			<size>0.1192708 0.2842593</size>									
			<path>./avatar-run.gif</path>
			<rotation>8</rotation>
			<flipX>true</flipX>
			
			<storyboard repeat="forever">
				<animation property="opacity" to="1" begin="2000" duration="10" mode="linear"/>
				<animation property="x" to="0.12" begin="2000" duration="400" mode="linear"/>
				<animation property="opacity" to="0" begin="2400" duration="10" mode="linear"/>
				
				<animation property="opacity" to="0" begin="16000" duration="10" mode="linear"/>
			</storyboard>
		</image>	
	
	
		<image name="avatar-surprised" extra="true">
			<pos>0.12 0.409</pos>
			<size>0.1296875 0.2694444</size>									
			<path>./avatar-surprised.png</path>
			
			<storyboard repeat="forever">
				<animation property="opacity" to="1" begin="2400" duration="10" mode="linear"/>
				<animation property="opacity" to="0" begin="3100" duration="10" mode="linear"/>
				
				<animation property="opacity" to="0" begin="16000" duration="10" mode="linear"/>
			</storyboard>
		</image>
	
	
		<image name="avatar-stand" extra="true">
			<pos>0.12 0.401</pos>
			<size>0.1192708 0.2842593</size>									
			<path>./avatar-stand.png</path>
			<rotation>3</rotation>
			
			<storyboard repeat="forever">
				<animation property="opacity" to="1" begin="3100" duration="10" mode="linear"/>
				<animation property="opacity" to="0" begin="5700" duration="10" mode="linear"/>
				
				<animation property="opacity" to="0" begin="16000" duration="10" mode="linear"/>
			</storyboard>
		</image>
		
		
		
		<image name="avatar-general_jump, avatar-general_stand">
			<origin>0 0</origin>
			<zIndex>3000</zIndex>
		</image>

		
		<image name="avatar-general_jump" extra="true">
			<pos>1.1 -0.3</pos>
			<size>0.1489583 0.3685185</size>									
			<scale>1</scale>
			<path>./avatar-general_jump.png</path>
			<opacity>1</opacity>
			<rotation>3</rotation>
			
			<storyboard repeat="forever">
				<animation property="x" to="0.5" begin="800" duration="1200" mode="linear"/>
				<animation property="y" to="0.3" begin="800" duration="1200" mode="bump"/>
				<animation property="scale" to="0.5" begin="800" duration="1200" mode="easeIn"/>
				
				<animation property="y" to="0.374" begin="2000" duration="1000" mode="linear"/>
				<animation property="opacity" to="0" begin="3000" duration="10" mode="linear"/>
				
				<animation property="opacity" to="1" begin="5100" duration="10" mode="linear"/>
				<animation property="y" to="0.24" begin="5100" duration="400" mode="easeInOut"/>
				<animation property="offsetX" to="0.03" begin="5300" duration="100" autoreverse="true" mode="easeInOut"/>
				
				<animation property="y" to="0.374" begin="5500" duration="200" mode="linear"/>
				<animation property="opacity" to="0" begin="5700" duration="10" mode="linear"/>
				
				<animation property="opacity" to="1" begin="7000" duration="10" mode="linear"/>
				<animation property="y" to="0.377" begin="7000" duration="50" mode="linear"/>
				<animation property="y" to="0.1" begin="7050" duration="1000" mode="bump"/>
				<animation property="x" to="-0.1" begin="7050" duration="1000" mode="linear"/>
				
				
				<animation property="opacity" to="0" begin="16000" duration="10" mode="linear"/>
			</storyboard>
		</image>
		
		
		
		<image name="avatar-general_stand" extra="true">
			<pos>0.5 0.377</pos>
			<size>0.1265625 0.3324074</size>									
			<scale>0.5</scale>
			<path>./avatar-general_stand.png</path>
			<opacity>0</opacity>
			
			<storyboard repeat="forever">
				<animation property="opacity" to="1" begin="3000" duration="10" mode="linear"/>
				<animation property="opacity" to="0" begin="5100" duration="10" mode="linear"/>
				<animation property="opacity" to="1" begin="5700" duration="10" mode="linear"/>
				<animation property="opacity" to="0" begin="7000" duration="10" mode="linear"/>
				
				
				<animation property="opacity" to="0" begin="16000" duration="10" mode="linear"/>
			</storyboard>
		</image>
		
		
		<image name="bloc_jaune, bloc_bleu, bloc_orange, bloc_vert">
			<origin>0 0</origin>
			<scale>0.5</scale>
			<zIndex>12</zIndex>
		</image>	
		
		
		<image name="bloc_vert" extra="true">
			<pos>0.1 -0.1</pos>
			<size>0.1442708 0.1759259</size>									
			<path>./bloc_vert.png</path>
			
			<storyboard repeat="forever">
				<animation property="y" from="-0.1" to="1" begin="" duration="2000" mode="linear"/>
				
				<animation property="rotation" to="90" begin="3000" duration="10" mode="linear"/>
				<animation property="x" to="0.3" begin="3000" duration="10" mode="linear"/>
				<animation property="y" from="-0.1" to="1" begin="3000" duration="2000" mode="linear"/>
				
				<animation property="rotation" to="0" begin="8000" duration="10" mode="linear"/>
				<animation property="x" to="0.18" begin="8000" duration="10" mode="linear"/>
				<animation property="y" from="-0.1" to="1" begin="8000" duration="2000" mode="linear"/>
				
				<animation property="rotation" to="90" begin="12000" duration="10" mode="linear"/>
				<animation property="x" to="0.33" begin="12000" duration="10" mode="linear"/>
				<animation property="y" from="-0.1" to="1" begin="12000" duration="2000" mode="linear"/>
				
				<animation property="opacity" to="0" begin="16000" duration="10" mode="linear"/>				
			</storyboard>
		</image>	

		
		<image name="bloc_jaune" extra="true">
			<pos>0.2 -0.1</pos>
			<size>0.1442708 0.1759259</size>									
			<path>./bloc_jaune.png</path>
			
			<storyboard repeat="forever">
				<animation property="y" from="-0.1" to="1" begin="1000" duration="2000" mode="linear"/>
				
				<animation property="rotation" to="90" begin="7000" duration="10" mode="linear"/>
				<animation property="x" to="0.5" begin="7000" duration="10" mode="linear"/>
				<animation property="y" from="-0.1" to="1" begin="7000" duration="2000" mode="linear"/>
				
				<animation property="rotation" to="0" begin="11000" duration="10" mode="linear"/>
				<animation property="x" to="0.18" begin="11000" duration="10" mode="linear"/>
				<animation property="y" from="-0.1" to="1" begin="11000" duration="2000" mode="linear"/>
				
				<animation property="opacity" to="0" begin="16000" duration="10" mode="linear"/>				
			</storyboard>
		</image>		
		
		<image name="bloc_bleu" extra="true">
			<pos>0.35 -0.1</pos>
			<size>0.196875 0.1861111</size>									
			<path>./bloc_bleu.png</path>
			
			<storyboard repeat="forever">
				<animation property="y" from="-0.1" to="1" begin="2000" duration="2000" mode="linear"/>
				<animation property="rotation" to="180" begin="6000" duration="10" mode="linear"/>
				<animation property="x" to="0.13" begin="6000" duration="10" mode="linear"/>
				<animation property="y" from="-0.1" to="1" begin="6000" duration="2000" mode="linear"/>
				
				<animation property="rotation" to="0" begin="10000" duration="10" mode="linear"/>
				<animation property="x" to="0.48" begin="10000" duration="10" mode="linear"/>
				<animation property="y" from="-0.1" to="1" begin="10000" duration="2000" mode="linear"/>
				
				<animation property="rotation" to="180" begin="14000" duration="10" mode="linear"/>
				<animation property="x" to="0.39" begin="14000" duration="10" mode="linear"/>
				<animation property="y" from="-0.1" to="1" begin="14000" duration="2000" mode="linear"/>
				
				<animation property="opacity" to="0" begin="16000" duration="10" mode="linear"/>				
			</storyboard>
		</image>	
		
		<image name="bloc_orange" extra="true">
			<pos>0.45 -0.1</pos>
			<size>0.1526042 0.2407407</size>									
			<path>./bloc_orange.png</path>
			
			<storyboard repeat="forever">
				<animation property="y" from="-0.1" to="0.25" begin="4500" duration="1000" mode="linear"/>
				
				<animation property="y" to="0.43" begin="5500" duration="800" mode="linear"/>
				<animation property="x" to="-0.1" begin="5500" duration="800" mode="linear"/>
				
				<animation property="rotation" to="180" begin="9000" duration="10" mode="linear"/>
				<animation property="x" to="0.3" begin="9000" duration="10" mode="linear"/>
				<animation property="y" from="-0.1" to="1" begin="9000" duration="2000" mode="linear"/>
				
				<animation property="rotation" to="0" begin="13000" duration="10" mode="linear"/>
				<animation property="x" to="0.21" begin="13000" duration="10" mode="linear"/>
				<animation property="y" from="-0.1" to="1" begin="13000" duration="2000" mode="linear"/>
				
				<animation property="opacity" to="0" begin="16000" duration="10" mode="linear"/>				
			</storyboard>
		</image>	
			
		
		
		
	</view>

</theme>
