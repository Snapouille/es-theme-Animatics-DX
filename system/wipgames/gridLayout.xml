<!-- Collection WIP Games -->


<theme>
	<formatVersion>7</formatVersion>

	<view name="system">
	
	    <image name="BG_snapLegacy" extra="true">
			<pos>0.5 0.5</pos>		
			<size>1 1</size>									
			<path>./background.png</path>
		</image>
		
	    <image name="start-warped, stop-warped, warped, space, arm1, arm2, box" extra="true">
			<origin>0.5 0.5</origin>
			<pos>0.5 0.5</pos>		
			<linearSmooth>true</linearSmooth>
			<scale>1</scale>
		</image>
		
		
	    <image name="warped" extra="true">
			<path>./warped.gif</path>
			<size>1 1</size>									
			<opacity>0</opacity>
			<zIndex>2</zIndex>
			<storyboard repeat="forever">
				<animation property="opacity" to="1" begin="3500" duration="1000" mode="linear"/>
				<animation property="opacity" to="0" begin="9500" duration="1000" mode="linear"/>
				
				<animation property="opacity" to="1" begin="20000" 	duration="1000" mode="linear"/>

				<animation property="opacity" to="0" begin="28000" duration="10" mode="linear"/>
				<animation property="opacity" to="0" begin="32000" duration="1"  mode="linear"/>
			</storyboard>
		</image>
	
	    <image name="space" extra="true">
			<origin>0.5 0.5</origin>									
			<size>2.0833333 0.6583333</size>									
			<pos>1.0296875 0.3291667</pos>	
			<opacity>1</opacity>
			<zIndex>1</zIndex>
		
			<path>./space.jpg</path>
			<storyboard repeat="forever">
				<animation property="offsetX" to="-0.03" 		begin="" 		duration="3000" mode="linear"/>
				<animation property="scale" to="3"  			begin="3000" 	duration="1500" mode="linear"/>
				<animation property="offsetX" to="0.33" 		begin="3000" 	duration="1500" mode="linear"/>
				<animation property="opacity" to="0" 			begin="3500" 	duration="1000" mode="linear"/>
