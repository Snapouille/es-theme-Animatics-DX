				<animation property="opacity" to="1" 			begin="10000" 	duration="1000" mode="linear"/>
				<animation property="scale" to="1"  			begin="10000" 	duration="500" mode="linear"/>
				<animation property="offsetX" to="-1" 			begin="10000" 	duration="500" mode="linear"/>
				<animation property="offsetX" to="-1.02" 		begin="10500" 	duration="9000" mode="linear"/>
				
				<animation property="scale" to="3"  			begin="19510" 	duration="1500" mode="linear"/>
				<animation property="offsetX" to="-2.1" 		begin="19510" 	duration="1500" mode="linear"/>
				<animation property="opacity" to="0" 			begin="20000" 	duration="1000" mode="linear"/>
				<animation property="offsetX" to="0.03"	 		begin="22100" 	duration="10" 	mode="linear"/>
				<animation property="scale" to="0.9"  			begin="22100" 	duration="10" 	mode="linear"/>
				
				<animation property="opacity" to="1" 			begin="27500" 	duration="1500" mode="linear"/>
				<animation property="scale" to="1"  			begin="27500" 	duration="1500" mode="linear"/>
				<animation property="offsetX" to="0" 			begin="29000" 	duration="3000" mode="linear"/>
				
				<animation property="opacity" to="1" 			begin="32000" 	duration="1" 	mode="linear"/>
			</storyboard>
		</image>
		
		
		
		<image name="box" extra="true">
		  <path>{random:thumbnail}</path>
		  <color>FFFFFFFF</color>
		  <pos>0.82 0.32</pos>
		  <origin>0.5 0.5</origin>
		  <maxSize>0.28 0.4</maxSize>
		  <zIndex>12</zIndex>
		  <scale>0.1</scale>
		  <opacity>0</opacity>
		  <visible>true</visible>
		  
			<storyboard repeat="forever">
				<animation property="opacity" to="1" 			begin="11000" 	duration="50" mode="linear"/>
				<animation property="scale" to="0.8"  			begin="11000" 	duration="500" mode="linear"/>
				
				<animation property="scale" to="1"  			begin="12900" 	duration="200" mode="linear"/>
				
				<animation property="scale" 	to="0.1"  		begin="18000" 	duration="500" mode="linear"/>
				<animation property="rotation" 	to="1080"  		begin="18000" 	duration="500" mode="linear"/>
				<animation property="opacity" 	to="0" 			begin="18450" 	duration="50" mode="linear"/>
				
				<animation property="opacity" to="0" 			begin="32000" 	duration="1" 	mode="linear"/>
			</storyboard>
		</image>
		
		
