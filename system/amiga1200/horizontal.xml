<!-- Amiga -->


<theme>
	<formatVersion>7</formatVersion>

	<view name="system">
		<image name="avatar" extra="static">
			<pos>0.11 0.122</pos>
			<scale>0.65</scale>
			
					

		<storyboard>
				
				
				
				<animation property="x" to="0.25" begin="700" duration="2000" mode="EaseOut"/>
				
				<animation property="scale" from="0.65" to="0.85" begin="2700" duration="350" mode="Linear"/>
				<animation property="x" from="0.25" to="0.37" begin="2700" duration="375" mode="EaseOut"/>
				<animation property="y" from="0.122" to="0.01" begin="2700" duration="375" mode="EaseOut"/>

        		<animation property="opacity" repeatAt="400" begin="3800" from="1" to="0.8" duration="10" autoreverse="true" repeat="5" mode="linear"/>
        		<animation property="opacity" repeatAt="400" begin="4500" from="1" to="0.8" duration="5" autoreverse="true" repeat="3" mode="linear"/>
        		<animation property="opacity" repeatAt="400" begin="6500" from="1" to="0.8" duration="7" autoreverse="true" repeat="2" mode="linear"/>
        		<animation property="opacity" repeatAt="400" begin="7000" from="1" to="0.8" duration="5" autoreverse="true" repeat="4" mode="linear"/>
        		<animation property="opacity" repeatAt="400" begin="12000" from="1" to="0.8" duration="10" autoreverse="true" repeat="10" mode="linear"/>
		</storyboard>
		
		
		
		
		
		
		
		</image>
	</view>

</theme>
