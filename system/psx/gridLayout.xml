<!-- PSX -->

<theme>
	<formatVersion>7</formatVersion>

	<view name="system">
	
	
	
		<image name="BG_snapLegacy" extra="true">
			<size>1 1</size>									
			<pos>0.5 0.44</pos>	
			<scale>1</scale>	
			<pos ifHelpPrompts="false">0.5 0.5</pos>	
			<saturation>0.7</saturation>
			<path>./background.jpg</path>
			  <storyboard>
				<animation property="saturation" to="1" begin="" duration="500" mode="linear"/>
			  </storyboard>
		</image>
	
	
		<image name="crabEyes" extra="static">
			<visible>false</visible>
		</image>
	
	
	
		<image name="apple" extra="static">
			<origin>0 0</origin>									
			<size>0.0520833 0.1083333</size>									
			<pos>0.28 -1</pos>
			<scale>0.8</scale>
			<opacity>1</opacity>
			<rotation>-30</rotation>
			<path>./apple.png</path>
			<zIndex>9</zIndex>
			
			<storyboard repeat="forever">
				<animation property="y" from="-1" to="1" begin="7000" duration="4000" mode="EaseIn"/>
				<animation property="opacity" to="0" begin="11500" duration="500" mode="linear"/>
				<animation property="opacity" to="0" begin="13000" duration="250" mode="linear"/>
			</storyboard>
			
			
		</image>
		
		
		
		
		<image name="apple2" extra="static">
			<origin>0 0</origin>									
			<size>0.0520833 0.1083333</size>	
			<pos>0.08 -1</pos>
			<scale>1</scale>
			<opacity>1</opacity>
			<rotation>30</rotation>
			<path>./apple.png</path>
			<zIndex>9</zIndex>
			
			
			<storyboard repeat="forever">
				<animation property="y" from="-1" to="1" begin="3000" duration="2800" mode="EaseIn"/>
				<animation property="opacity" to="0" begin="6000" duration="500" mode="linear"/>
				<animation property="opacity" to="0" begin="11000" duration="250" mode="linear"/>
			</storyboard>
			
		</image>
	
	
		<image name="apple3" extra="static">
			<origin>0 0</origin>									
			<size>0.0520833 0.1083333</size>	
			<pos>0.45 -1</pos>
			<scale>0.9</scale>
			<opacity>1</opacity>
			<rotation>-10</rotation>
			<path>./apple.png</path>
			<zIndex>9</zIndex>
			
			<storyboard repeat="forever">
				<animation property="y" from="-1" to="1" begin="" duration="2500" mode="EaseIn"/>
				<animation property="opacity" to="0" begin="6300" duration="100" mode="linear"/>
				
				<animation property="opacity" to="0" begin="13000" duration="500" mode="linear"/>
			</storyboard>
			
		</image>
	
	
			
		<image name="chute" extra="static">
			<origin>0 0</origin>									
			<size>0.46875 0.4851852</size>									

			<pos>0.27 -1</pos>
			<scale>0.8</scale>
			<rotation>0</rotation>
			<opacity>1</opacity>
			<path>./chute.png</path>
			<zIndex>2100</zIndex>
		
			<storyboard>
				<animation property="y" from="-1" to="1" begin="" duration="2000" mode="linear"/>
			</storyboard>
		
		</image>

		
		
		<image name="crash" extra="static">
			<origin>0 0</origin>									
			<size>0.29375 0.7777778</size>									
			<pos>0.33 1</pos>
			<pos ifHelpPrompts="false">0.33 1.06</pos>	
			<scale>0.5</scale>
			<opacity>1</opacity>
			<path>./crash2.png</path>
			<zIndex>8</zIndex>
		
			<storyboard>
					<animation property="x" to="0.66" begin="3000" duration="1000" mode="linear"/>
					<animation property="offsetY" to="-0.72" begin="3000" duration="1000" mode="bump"/>
					<animation property="scale" to="0.85" begin="3000" duration="1000" mode="linear"/>
			</storyboard>
		</image>


		
	<image name="crash2" extra="static">
		<visible>false</visible>
		
		</image>





		
	</view>
	

</theme>
