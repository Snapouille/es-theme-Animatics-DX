<!-- Naomi -->


<theme>
	<formatVersion>7</formatVersion>

	<view name="system">
		<image name="avatar" extra="static">
			<origin>0 0</origin>									
			<size>0.1760417 1.0055556</size>									
			<pos>0.17 0.09</pos>
			<scale>0.65</scale>
			<opacity>0</opacity>
			<zIndex>18</zIndex>
		</image>	
	
	
	<image name="avatar2" extra="static">
			<origin>0 0</origin>									
			<size>0.0338542 0.1</size>									
			<pos>0.479 0.202</pos>
			<scale>0.98</scale>
			<path>./avatar2.png</path>
			<opacity>0</opacity>
			<zIndex>19</zIndex>
			<linearSmooth>true</linearSmooth>
	</image>
	
	
		<image name="avatar" extra="true">	
		
			<storyboard repeat="forever">
					
					<animation property="opacity" from="0" to="1" begin="" duration="100" mode="linear"/>
					<animation property="x" to="0.27" begin="100" duration="3700" mode="EaseOut"/>
					<animation property="scale" from="0.65" to="0.815" begin="4000" duration="350" mode="bump"/>
					<animation property="x"  to="0.45" begin="4000" duration="375" mode="EaseOut"/>
					<animation property="y"  to="0.09" begin="4000" duration="375" mode="EaseOut"/>

					
					<animation property="scale" to="7" begin="12800" duration="1500" mode="Linear"/>
					<animation property="opacity" to="0" begin="12800" duration="1500" mode="linear"/>
					<animation property="x" from="0.38" to="-0.1" begin="12800" duration="1500" mode="EaseOut"/>
					
					
			</storyboard>
		</image>
		
		
		<image name="avatar2" extra="true">	
			<storyboard repeat="forever">
					
					<animation property="opacity" from="0" to="1" begin="4375" duration="10" mode="linear"/>
					<animation property="rotation" from="2" to="-2" begin="4375" autoreverse="true" duration="400" repeat="10" mode="easyin"/>
					<animation property="y" from="0.204" to="0.203" begin="4375" autoreverse="true" duration="400" repeat="10" mode="easyin"/>
					<animation property="opacity" from="1" to="0" begin="12790" duration="10" mode="linear"/>
					<animation property="opacity" from="0" to="0" begin="12800" duration="1500" mode="linear"/>
			</storyboard>
		</image>
		
		
		
		
	</view>

</theme>
