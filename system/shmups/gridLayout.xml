<!-- Shumps collection -->

<theme>
	<formatVersion>7</formatVersion>
	
	<view name="system">

		<image name="avatar" extra="static">
			<origin>0.5 0.5</origin>									
			<size>0.29375 0.4481481</size>									
			<pos>1.05 1.55</pos>									
			<scale>1</scale>
			<linearSmooth>true</linearSmooth>	
			<flipX>true</flipX>
			<rotation>10</rotation>
		
			<storyboard>
				<animation property="offsetX" to="-0.22" begin="100" duration="1500" mode="EaseOutCubic"/>
				<animation property="offsetY" to="-1" begin="100" duration="1500" mode="EaseOutCubic"/>
				<animation property="rotation" to="-10" begin="100" duration="1500" mode="EaseOutCubic"/>
			</storyboard>

		</image>
		
	</view>
</theme>
