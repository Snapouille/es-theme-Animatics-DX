<!-- Nintendo 64 -->

<theme>
	<formatVersion>7</formatVersion>


	<view name="system" ifSubset="systemview:horizontal">
	
		<image name="link" extra="static">
			<pos>0.15 0.081</pos>
			<scale>1</scale>
			<opacity>1</opacity>
			<path>./link.png</path>
			<zIndex>170</zIndex>
			<linearSmooth>true</linearSmooth>
		
			<storyboard>
        		<animation property="x" to="0.1" begin="" autoreverse="true" duration="15000" repeat="forever" mode="EaseInOut"/>
			</storyboard>
		</image>
		
		<image name="fee" extra="true">
			<pos>0.19 0.081</pos>
			<scale>0.5</scale>
			<rotation>-15</rotation>
			<opacity>0</opacity>
			<path>./fee.gif</path>
			<zIndex>171</zIndex>
		
			<storyboard repeat="forever">
        		<animation property="x" to="0.11" begin="1000" autoreverse="true" duration="15000" mode="linear"/>
				
				<animation property="opacity" to="1" begin="1000" duration="500" mode="linear"/>
				<animation property="scale" to="0.8" begin="1000" duration="2000" mode="Linear"/>
				<animation property="y" from="-0.3" to="0.20" begin="1000" duration="2000" mode="easeOut"/>
				
				<animation property="rotation" to="-13" begin="1000" autoreverse="true" duration="650" repeat="14" mode="bump"/>
				<animation property="y" from="0.20" to="0.21" begin="3000" autoreverse="true" duration="400" repeat="16" mode="easeOut"/>

				<animation property="scale" to="1.2" begin="16000" duration="2500" mode="Linear"/>
				<animation property="x" to="-0.2" begin="16000" duration="2500" mode="easeIn"/>
				<animation property="y" to="-0.3" begin="16000" duration="2500" mode="easeIn"/>
				<animation property="opacity" to="0" begin="18500" duration="500" mode="linear"/>
			</storyboard>
		</image>
		
		<image name="bird1, bird2, bird3, bird4, bird5, bird6, bird7, bird8, bird9" extra="true">
			<origin>0.5 0.5</origin>			
			<size>0.0395833 0.0509259</size>	
			<path>./bird.gif</path>
			<opacity>0</opacity>
			<zIndex>7</zIndex>
		</image>
		
		
		<image name="bird1" extra="true">
			<pos>0.6609375 0.275463</pos>									
			<storyboard repeat="forever">
        		<animation property="opacity" to="1" 	begin="5000" duration="10" mode="linear"/>
        		<animation property="offsetX" to="-0.4" begin="5000" duration="5000" mode="bump"/>
        		<animation property="offsetY" to="-0.4" begin="5000" duration="5000" mode="linear"/>
        		<animation property="scale"   to="0.55" begin="5000" duration="5000" mode="linear"/>
        		<animation property="opacity" to="0" 	begin="25000" duration="10" mode="linear"/>
			</storyboard>
		</image>
		
		<image name="bird2" extra="true">
			<pos>0.6809375 0.275463</pos>									
			<scale>0.8</scale>
			<storyboard repeat="forever">
        		<animation property="opacity" to="1" 	begin="5200" duration="10" mode="linear"/>
        		<animation property="offsetX" to="-0.5" begin="5200" duration="5000" mode="bump"/>
        		<animation property="offsetY" to="-0.4" begin="5200" duration="5000" mode="linear"/>
        		<animation property="scale"   to="0.45" begin="5200" duration="5000" mode="linear"/>
        		<animation property="opacity" to="0" 	begin="25100" duration="10" mode="linear"/>
			</storyboard>
		</image>
		
		<image name="bird3" extra="true">
			<pos>0.7609375 0.275463</pos>									
			<scale>0.6</scale>
			<storyboard repeat="forever">
        		<animation property="opacity" to="1" 	begin="5300" duration="10" mode="linear"/>
        		<animation property="offsetX" to="-0.55" begin="5300" duration="5000" mode="bump"/>
        		<animation property="offsetY" to="-0.4" begin="5300" duration="5000" mode="linear"/>
        		<animation property="scale"   to="0.25" begin="5300" duration="5000" mode="linear"/>
        		<animation property="opacity" to="0" 	begin="25200" duration="10" mode="linear"/>
			</storyboard>
		</image>
		
		<image name="bird4" extra="true">
			<pos>0.7609375 0.28</pos>									
			<scale>0.5</scale>
			<storyboard repeat="forever">
        		<animation property="opacity" to="1" 	begin="5500" duration="10" mode="linear"/>
        		<animation property="offsetX" to="-0.55" begin="5500" duration="5000" mode="bump"/>
        		<animation property="offsetY" to="-0.4" begin="5500" duration="5000" mode="linear"/>
        		<animation property="scale"   to="0.20" begin="5500" duration="5000" mode="linear"/>
        		<animation property="opacity" to="0" 	begin="25200" duration="10" mode="linear"/>
			</storyboard>
		</image>
		
		<image name="bird5" extra="true">
			<pos>0.7609375 0.30</pos>									
			<scale>0.3</scale>
			<storyboard repeat="forever">
        		<animation property="opacity" to="1" 	begin="5600" duration="10" mode="linear"/>
        		<animation property="offsetX" to="-0.55" begin="5600" duration="5000" mode="bump"/>
        		<animation property="offsetY" to="-0.4" begin="5600" duration="5000" mode="linear"/>
        		<animation property="scale"   to="0.15" begin="5600" duration="5000" mode="linear"/>
        		<animation property="opacity" to="0" 	begin="25200" duration="10" mode="linear"/>
			</storyboard>
		</image>
		
		<image name="bird6" extra="true">
			<pos>0.7609375 0.31</pos>									
			<scale>0.4</scale>
			<storyboard repeat="forever">
        		<animation property="opacity" to="1" 	begin="5800" duration="10" mode="linear"/>
        		<animation property="offsetX" to="-0.55" begin="5800" duration="5000" mode="bump"/>
        		<animation property="offsetY" to="-0.4" begin="5800" duration="5000" mode="linear"/>
        		<animation property="scale"   to="0.2" begin="5800" duration="5000" mode="linear"/>
        		<animation property="opacity" to="0" 	begin="25200" duration="10" mode="linear"/>
			</storyboard>
		</image>
		
		<image name="bird7" extra="true">
			<pos>0.7809375 0.30</pos>									
			<scale>0.5</scale>
			<storyboard repeat="forever">
        		<animation property="opacity" to="1" 	begin="6000" duration="10" mode="linear"/>
        		<animation property="offsetX" to="-0.55" begin="6000" duration="5000" mode="bump"/>
        		<animation property="offsetY" to="-0.4" begin="6000" duration="5000" mode="linear"/>
        		<animation property="scale"   to="0.3" begin="6000" duration="5000" mode="linear"/>
        		<animation property="opacity" to="0" 	begin="25200" duration="10" mode="linear"/>
			</storyboard>
		</image>
		
		
		<image name="bird8" extra="true">
			<pos>0 0.375463</pos>									
			<flipX>true</flipX>
			<storyboard repeat="forever">
        		<animation property="opacity" to="1" 	begin="11000" duration="10" mode="linear"/>
        		<animation property="offsetX" to="0.16" begin="11000" duration="5000" mode="bump"/>
        		<animation property="offsetY" to="-0.5" begin="11000" duration="5000" mode="linear"/>
        		<animation property="scale"   to="0.35" begin="11000" duration="5000" mode="linear"/>
        		<animation property="opacity" to="0" 	begin="30000" duration="10" mode="linear"/>
			</storyboard>
		</image>
		
		<image name="bird9" extra="true">
			<pos>0 0.375463</pos>									
			<scale>0.8</scale>
			<flipX>true</flipX>
			<storyboard repeat="forever">
        		<animation property="opacity" to="1" 	begin="11300" duration="10" mode="linear"/>
        		<animation property="offsetX" to="0.16" begin="11300" duration="5000" mode="bump"/>
        		<animation property="offsetY" to="-0.5" begin="11300" duration="5000" mode="linear"/>
        		<animation property="scale"   to="0.25" begin="11300" duration="5000" mode="linear"/>
        		<animation property="opacity" to="0" 	begin="30000" duration="10" mode="linear"/>
			</storyboard>
		</image>		
				
	</view>

</theme>
