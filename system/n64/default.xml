<!-- Nintendo 64 -->

<theme>
	<formatVersion>7</formatVersion>

	<view name="system">
	
		<image name="link" extra="static">
			<pos>0.15 0.08</pos>
			<scale>0.9</scale>
			<opacity>1</opacity>
			<path>./link.png</path>
			<zIndex>17</zIndex>
		
			<storyboard>
        		<animation property="x" to="0.05" begin="" autoreverse="true" duration="15000" repeat="forever" mode="linear"/>
			</storyboard>
		</image>
		
		<image name="fee" extra="true">
			<pos>0.15 0.081</pos>
			<scale>0.4</scale>
			<rotation>-15</rotation>
			<opacity>0</opacity>
			<path>./fee.png</path>
			<zIndex>17</zIndex>
		
			<storyboard repeat="forever">
        		<animation property="x" to="0.05" begin="" autoreverse="true" duration="15000" mode="linear"/>
				
				<animation property="opacity" to="1" begin="" duration="500" mode="linear"/>
				<animation property="scale" to="0.7" begin="" duration="2000" mode="Linear"/>
				<animation property="y" from="-0.3" to="0.20" begin="" duration="2000" mode="easeOut"/>
				
				<animation property="rotation" to="-13" begin="" autoreverse="true" duration="650" repeat="14" mode="linear"/>
				<animation property="y" from="0.20" to="0.21" begin="2000" autoreverse="true" duration="400" repeat="16" mode="easeOut"/>

				<animation property="scale" to="1.3" begin="15000" duration="2500" mode="Linear"/>
				<animation property="x" to="-0.2" begin="15000" duration="2500" mode="easeIn"/>
				<animation property="y" to="-0.3" begin="15000" duration="2500" mode="easeIn"/>
				<animation property="opacity" to="0" begin="17500" duration="500" mode="linear"/>
			</storyboard>
		</image>
	</view>


	

</theme>
