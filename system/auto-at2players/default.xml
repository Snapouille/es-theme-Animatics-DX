<!-- Auto 2 Players -->


<theme>
	<formatVersion>7</formatVersion>

	<view name="system">
	
		<image name="avatar2p" extra="true">
			<origin>0 0</origin>									
			<size>0.4036458 0.4453704</size>									
			<pos>0.213 0.33</pos>
			<opacity>1</opacity>
			<path>./avatar2p.png</path>
			<linearSmooth>true</linearSmooth>
			<zIndex>19</zIndex>

		<storyboard repeat="forever">
				
				<animation property="opacity" to="0" begin="300" duration="10" mode="linear"/>
				<animation property="opacity" to="1" begin="600" duration="10" mode="linear"/>
				
        		
		</storyboard>
		
		</image>
		
		
		<image name="avatarbis" extra="true">
			<origin>0 0</origin>									
			<size>0.4036458 0.4453704</size>									
			<pos>0.213 0.33</pos>
			<opacity>0</opacity>
			<linearSmooth>true</linearSmooth>
			<path>./avatarbis.png</path>
			<zIndex>19</zIndex>	
				
		<storyboard repeat="forever">
				
				<animation property="opacity" to="1" begin="300" duration="10" mode="linear"/>
				<animation property="opacity" to="0" begin="600" duration="10" mode="linear"/>
				
		</storyboard>
		
		</image>
		
		
		<image name="plane" extra="true">
			<origin>0.5 0.5</origin>									
			<size>1.1760417 0.6425926</size>									
			<pos>0.2885417 -0.0231481</pos>									
			<scale>0</scale>
			<opacity>1</opacity>
			<linearSmooth>true</linearSmooth>
			<path>./plane.png</path>
			<zIndex>16</zIndex>	
				
			<storyboard>
				
				<animation property="offsetX" from="-0.1" to="0" begin="5000" duration="10000" mode="linear"/>
				<animation property="offsetY" from="0.25" to="-0.51" begin="5000" duration="3500" mode="linear"/>
				<animation property="rotation" to="-20" begin="5500" duration="2000" mode="linear"/>
				<animation property="scale" to="1" begin="5000" duration="10000" mode="bump"/>
				<animation property="opacity" to="0" begin="8500" duration="10" mode="linear"/>
				
			</storyboard>
		</image>
		
		
	</view>

</theme>
