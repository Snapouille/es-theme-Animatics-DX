<!-- Super Acan -->

<theme>
	<formatVersion>7</formatVersion>

	<view name="system">
	
		
		<image name="arcenciel" extra="static">
			<pos>-0.2 0</pos>
			<scale>1</scale>
			<opacity>1</opacity>
			<path>./arcenciel.png</path>
			<zIndex>16</zIndex>
		
			<storyboard repeat="forever">
				<animation property="opacity" from="0.8" to="1" begin="" duration="1000" autoreverse="true" repeat="2" mode="linear"/>
				<animation property="opacity" from="0.8" to="1" begin="5000" duration="100" autoreverse="true" repeat="1" mode="linear"/>
				<animation property="opacity" from="0.8" to="1" begin="5200" duration="200" autoreverse="true" repeat="1" mode="linear"/>
				<animation property="opacity" from="0.8" to="1" begin="5600" duration="500" autoreverse="true" repeat="2" mode="linear"/>
				<animation property="opacity" from="0.8" to="1" begin="7600" duration="200" autoreverse="true" repeat="2" mode="linear"/>
			</storyboard>
		
		</image>
		
		
		
		
		
		
		<image name="avatar1" extra="static">
			<pos>0.31 0.27</pos>
			<scale>0.9</scale>
			<opacity>1</opacity>
			<path>./avatar1.png</path>
			<zIndex>17</zIndex>


			<storyboard repeat="forever">
					
