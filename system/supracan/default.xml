					<animation property="y" to="0.268" begin="" duration="455" autoreverse="true" mode="EaseInOut"/>
					<animation property="scale" to="0.898" begin="" duration="455" autoreverse="true" mode="Bump"/>
					
					<animation property="opacity" to="0" begin="150" duration="10" mode="linear"/>
					<animation property="opacity" to="1" begin="450" duration="10" mode="linear"/>
					
					<animation property="opacity" to="0" begin="600" duration="10" mode="linear"/>
					<animation property="opacity" to="1" begin="900" duration="10" mode="linear"/>
					
			</storyboard>
		</image>
		
	
