<!-- Flash -->

<theme>
	<formatVersion>7</formatVersion>

	<view name="system">
	
		
		<image name="avatar-yeti2" extra="static">
			<pos>0.3 0.26</pos>
			<scale>1</scale>
			<opacity>1</opacity>
			<path>./avatar-yeti2.png</path>
			<zIndex>14</zIndex>
			<storyboard repeat="forever">
				<animation property="opacity" from="1" to="0" begin="5000" duration="100" mode="linear"/>
				<animation property="opacity" to="1" begin="15000" duration="500" mode="linear"/>
			</storyboard>
		</image>
		
		<image name="avatar-yeti1" extra="static">
			<pos>0.3 0.26</pos>
			<scale>1</scale>
			<opacity>0</opacity>
			<path>./avatar-yeti1.png</path>
			<zIndex>14</zIndex>
			<storyboard repeat="forever">
				<animation property="opacity" from="0" to="1" begin="5000" duration="100" mode="linear"/>
				<animation property="opacity" to="0" begin="15000" duration="500" mode="linear"/>
			</storyboard>
		</image>
		
		
		
	</view>

</theme>
