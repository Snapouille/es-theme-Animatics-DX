			  <storyboard>
				<animation property="saturation" to="1" begin="" duration="500" mode="linear"/>
			  </storyboard>
		</image>








		<image name="avatar" extra="static">
			<origin>0.5 0.5</origin>									
			<size>0.2322917 0.7546296</size>									
			<pos>0.43 0.80</pos>	
			<scale>0.6</scale>
