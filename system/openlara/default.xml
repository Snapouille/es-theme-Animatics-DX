<!-- OpenLara -->

<theme>
	<formatVersion>7</formatVersion>
	
	<view name="system">

		<image name="avatar" extra="static">
			<origin>0.5 0.5</origin>	
			<size>0.2604167 0.8333333</size>									
			<pos>0.37 0.55</pos>	
			<scale>0.9</scale>
			<linearSmooth>true</linearSmooth>					
			<storyboard>
				
        		<animation property="x" to="0.51" begin="" duration="1000" mode="EaseInOut"/>
				<animation property="scale" to="1" begin="" duration="1000" mode="EaseInOut"/>
        		<animation property="y" to="0.45" begin="" duration="1000" mode="EaseInOut"/>
				
			</storyboard>					

		</image>
		
	</view>
</theme>
