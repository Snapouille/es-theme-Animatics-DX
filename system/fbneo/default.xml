			<origin>0 0</origin>
			<pos>0.2 0.167</pos>
			<size>0.2661458 0.7407407</size>									


			<scale>0.8</scale>
			<zIndex>16</zIndex>
			<opacity>1</opacity>
			<path>./avatar-punisher.png</path>
			
			<storyboard repeat="forever">
			
			
				<animation property="x" to="0.01" begin="" duration="200" mode="EaseOut"/>
				<animation property="x" to="0.13" begin="200" duration="1800" mode="EaseOut"/>
