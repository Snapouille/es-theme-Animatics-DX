<!-- Astrocade -->

<theme>
	<formatVersion>7</formatVersion>
	
	<view name="system">
	

		<image name="avatar" extra="true">
			<scale>0.85</scale>
			<linearSmooth>true</linearSmooth>
			<size>0.2604167 0.7935185</size>									
			<storyboard>
				
				<animation property="offsetX" to="0.1" begin="" duration="1000" mode="EaseInOut"/>
				<animation property="offsetY" to="0.07" begin="" duration="10" mode="EaseIn"/>
				
			</storyboard>					

		</image>
		
		
	</view>
</theme>
