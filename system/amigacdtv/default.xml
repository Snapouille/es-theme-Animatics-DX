<!-- Amiga CdTv -->

<theme>
	<formatVersion>7</formatVersion>

	<view name="system">
		<image name="avatar" extra="static">
			<pos>0.027 0.191</pos>
			<scale>1</scale>
			<opacity>1</opacity>
	


			<storyboard>
					
				<animation property="x" to="0.028" begin="900" autoreverse="true" duration="1000" repeat="forever" mode="easyIn"/>
        		        						
			</storyboard>

		
		</image>
	</view>

</theme>
