<!-- j2me JAVA  -->


<theme>
	<formatVersion>7</formatVersion>

	<view name="system">
		<image name="avatar" extra="true">
			<origin>0.5 0.5</origin>
			<size>0.1677083 0.7287037</size>									
			<pos>0.58 0.4777778</pos>	
			<path>./avatar.png</path>

			<storyboard>
				<animation property="offsetX" to="-0.05" begin="" duration="1000" mode="EaseOut"/>
			</storyboard>
			
			<storyboard ifHelpPrompts="false">
				<animation property="offsetX" to="-0.05" begin="" duration="1000" mode="EaseOut"/>
			</storyboard>
		
		</image>
	</view>

</theme>
