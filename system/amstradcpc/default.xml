<!-- Amstrad CPC -->

<theme>
	<formatVersion>7</formatVersion>

	<view name="system">
	
		<image name="background-city" extra="static"> 
			<pos>0 0</pos>
			<scale>1</scale>
			<path>./background-city.png</path>
			<zIndex>13</zIndex>
		</image>
		
		
		<image name="background-moonmask" extra="true"> 
			<pos>0 0</pos>
			<scale>1</scale>
			<path>./background-moonmask.png</path>
			<zIndex>14</zIndex>
		</image>
		
		<image name="background-moon" extra="static"> 
			<pos>0.22 0.1</pos>
			<scale>0.8</scale>
			<path>./background-moon.png</path>
			<zIndex>13</zIndex>
			
			<storyboard repeat="forever">
				<animation property="y" to="0.25" begin="" duration="45000" mode="linear"/>
				
				<animation property="opacity" to="0" begin="59500" duration="500" mode="linear"/>
			</storyboard>
			
		</image>
		
		<image name="background-beacon" extra="true"> 
			<pos>0.46171 0.1725</pos>
			<scale>0.7</scale>
			<path>./background-beacon.png</path>
			<opacity>0</opacity>
			<zIndex>13</zIndex>
			
			<storyboard repeat="forever">
					<animation property="opacity" from="0" to="1" begin="" duration="2000" autoreverse="true" mode="linear"/>
			</storyboard>
			
		</image>
		
	
		<image name="background-windows1" extra="true"> 
			<pos>0 0</pos>
			<scale>1</scale>
			<opacity>1</opacity>
			<path>./background-windows1.png</path>
			<zIndex>14</zIndex>
			
			<storyboard repeat="forever">
			
					<animation property="opacity" from="1" to="0" begin="18000" duration="10" mode="linear"/>
					
					<animation property="opacity" to="0" begin="59500" duration="500" mode="linear"/>
			</storyboard>	
			
		</image>
	
		<image name="background-windows2" extra="true"> 
			<pos>0 0</pos>
			<scale>1</scale>
			<opacity>0</opacity>
			<path>./background-windows2.png</path>
			<zIndex>14</zIndex>
			
			<storyboard repeat="forever">
					<animation property="opacity" from="0" to="1" begin="18000" duration="10" mode="linear"/>
					<animation property="opacity" from="1" to="0" begin="33000" duration="10" mode="linear"/>
					
					<animation property="opacity" to="0" begin="59500" duration="500" mode="linear"/>
			</storyboard>	
			
		</image>
	
		
		<image name="background-windows3" extra="true"> 
			<pos>0 0</pos>
			<scale>1</scale>
			<opacity>0</opacity>
			<path>./background-windows3.png</path>
			<zIndex>14</zIndex>
			
			<storyboard repeat="forever">
					<animation property="opacity" from="0" to="1" begin="33000" duration="10" mode="linear"/>
					<animation property="opacity" from="1" to="0" begin="48000" duration="10" mode="linear"/>
					
					<animation property="opacity" to="0" begin="59500" duration="500" mode="linear"/>
			</storyboard>	
			
		</image>
		
	
	
	
		<image name="avatar-girl1" extra="static">
			<pos>0.2 0.23</pos>
			<scale>0.6</scale>
			<opacity>1</opacity>
			<path>./avatar-girl1.png</path>
			<zIndex>14</zIndex>
			<storyboard repeat="forever">
				<animation property="x" to="0.42" begin="" duration="3000" mode="linear"/>
				<animation property="opacity" from="1" to="0" begin="3000" duration="50" mode="linear"/>
				<animation property="x" to="0.42" begin="3000" duration="3000" mode="linear"/>
				<animation property="scale" to="0.65" begin="3000" duration="3000" mode="linear"/>
				
				<animation property="x" to="0.40" begin="6000" duration="3000" autoreverse="true" repeat="9" mode="EaseIn"/>
				
				<animation property="opacity" from="0" to="1" begin="18000" duration="50" mode="linear"/>
				<animation property="opacity" from="1" to="0" begin="20000" duration="50" mode="linear"/>
				
				<animation property="opacity" from="0" to="1" begin="38000" duration="50" mode="linear"/>
				<animation property="opacity" from="1" to="0" begin="40000" duration="50" mode="linear"/>
				
				<animation property="opacity" to="0" begin="59500" duration="500" mode="linear"/>
			</storyboard>
		</image>
		
		<image name="avatar-girl2" extra="static">
			<pos>0.2 0.23</pos>
			<scale>0.6</scale>
			<opacity>0</opacity>
			<path>./avatar-girl2.png</path>
			<zIndex>14</zIndex>
			<storyboard repeat="forever">
				<animation property="x" to="0.42" begin="" duration="3000" mode="linear"/>
				<animation property="opacity" from="0" to="1" begin="3000" duration="50" mode="linear"/>
				<animation property="x" to="0.42" begin="3000" duration="3000" mode="linear"/>
				<animation property="scale" to="0.65" begin="3000" duration="3000" mode="linear"/>
				
				
				<animation property="x" to="0.40" begin="6000" duration="3000" autoreverse="true" repeat="9" mode="EaseIn"/>
				
				<animation property="opacity" from="1" to="0" begin="18000" duration="50" mode="linear"/>
				<animation property="opacity" from="0" to="1" begin="20000" duration="50" mode="linear"/>
				
				<animation property="opacity" from="1" to="0" begin="38000" duration="50" mode="linear"/>
				<animation property="opacity" from="0" to="1" begin="40000" duration="50" mode="linear"/>
				
				<animation property="scale" to="1" begin="53000" duration="7000" mode="linear"/>
				<animation property="y" to="0.21" begin="53000" duration="6000" mode="linear"/>
				
				<animation property="opacity" to="0" begin="59400" duration="500" mode="linear"/>
				<animation property="opacity" to="0" begin="59900" duration="100" mode="linear"/>
				
			</storyboard>
		</image>
		
		
		
		
		
		<image name="avatar-billy1" extra="static">
			<pos>0.38 0.23</pos>
			<scale>0.8</scale>
			<opacity>1</opacity>
			<path>./avatar-billy.png</path>
			<zIndex>14</zIndex>
			<storyboard repeat="forever">
				<animation property="x" to="0.18" begin="" duration="2500" mode="linear"/>
				<animation property="opacity" from="1" to="0" begin="2400" duration="100" mode="linear"/>
				
				
				<animation property="opacity" to="0" begin="59500" duration="500" mode="linear"/>
			</storyboard>
		</image>
		
		<image name="avatar-billy2" extra="static">
			<pos>0.38 0.23</pos>
			<scale>0.8</scale>
			<opacity>0</opacity>
			<path>./avatar-billy.png</path>
			<flipX>true</flipX>
			<zIndex>14</zIndex>
			<storyboard repeat="forever">
				<animation property="x" to="0.18" begin="" duration="2500" mode="linear"/>
				<animation property="opacity" from="0" to="1" begin="2400" duration="100" mode="linear"/>
				<animation property="x" to="0.22" begin="2500" duration="6500" autoreverse="true" repeat="3" mode="EaseOutCubic"/>
				<animation property="x" to="0.31" begin="41500" duration="18500" mode="EaseOutCubic"/>
				
				<animation property="opacity" to="0" begin="59500" duration="500" mode="linear"/>
			</storyboard>
		</image>
		
		
		<image name="avatar-trash" extra="static">
			<pos>0.09 0.467</pos>
			<scale>0.6</scale>
			<opacity>1</opacity>
			<path>./avatar-trash.png</path>
			<zIndex>14</zIndex>
			<storyboard repeat="forever">
				<animation property="x" to="0.06" begin="" duration="2000" mode="EaseOutCubic"/>
				
				<animation property="opacity" from="1" to="0" begin="59500" duration="500" mode="linear"/>
			</storyboard>
		</image>
		
		
		<image name="avatar-heart" extra="static">
			<pos>0.43 0.45</pos>
			<scale>0.1</scale>
			<opacity>0</opacity>
			<path>./avatar-heart.png</path>
			<zIndex>50</zIndex>
			
			<storyboard repeat="forever">
					
					<animation property="opacity" to="1" begin="59200" duration="100" mode="linear"/>
					<animation property="x" to="0.07" begin="59200" duration="800" mode="linear"/>
					<animation property="y" to="-0.1" begin="59200" duration="800" mode="linear"/>
					<animation property="scale" to="1" begin="59200" duration="800" mode="linear"/>
				
			</storyboard>
		</image>
		
		
		
		
		
		
		
		
		
		
		<image name="nuage0" extra="static">
			<pos>0.37 0.10</pos>
			<scale>0.62</scale>
			<opacity>1</opacity>
			<path>./nuage0.png</path>
			<zIndex>16</zIndex>
		</image>
		
		
		<image name="nuage2" extra="static">
			<pos>0.18 0.12</pos>
			<scale>0.70</scale>
			<opacity>1</opacity>
			<path>./nuage2.png</path>
			<zIndex>16</zIndex>
		</image>
		
		<image name="nuage1" extra="static">
			<pos>0.01 0.11</pos>
			<scale>0.72</scale>
			<opacity>0</opacity>
			<path>./nuage1.png</path>
			<zIndex>16</zIndex>
		</image>
		
		<image name="nuage3" extra="static">
			<pos>0.35 0.13</pos>
			<scale>0.72</scale>
			<opacity>0</opacity>
			<path>./nuage3.png</path>
			<zIndex>16</zIndex>
		</image>
		
		
		
		
		
		
		
		<image name="nuage0" extra="true">
		
			<storyboard repeat="forever">
				<animation property="x" from="0.37" to="0.58" begin="" duration="35000" mode="easyOut"/>
				<animation property="opacity" from="1" to="0" begin="34000" duration="1000" mode="linear"/>
				
				<animation property="opacity" to="0" begin="59500" duration="500" mode="linear"/>
				
			</storyboard>			
		</image>
		
		<image name="nuage2" extra="true">
		
			<storyboard repeat="forever">
				<animation property="x" from="0.10" to="0.58" begin="" duration="55000" mode="easyOut"/>
				<animation property="opacity" from="1" to="0" begin="52000" duration="1000" mode="linear"/>
				
				<animation property="opacity" to="0" begin="59500" duration="500" mode="linear"/>
				
			</storyboard>			
		</image>
		
		
		<image name="nuage1" extra="true">
		
			<storyboard repeat="forever">
			
				<animation property="opacity" from="0" to="0.9" begin="10000" duration="8000" mode="linear"/>
				<animation property="x" from="-0.1" to="0.58" begin="10000" duration="50000" mode="easyOut"/>
				
				
				<animation property="opacity" to="0" begin="59500" duration="500" mode="linear"/>
				
			</storyboard>			
		</image>
		
		
		
		<image name="nuage3" extra="true">
		
			<storyboard repeat="forever">
			
				<animation property="opacity" from="0" to="0.92" begin="32000" duration="8000" mode="linear"/>
				<animation property="x" from="-0.1" to="0.45" begin="30000" duration="42000" mode="easyOut"/>
				
				
				<animation property="opacity" to="0" begin="59500" duration="500" mode="linear"/>
				
			</storyboard>	
			
		</image>
		
		
		
		
		
		
		
		
				
		
		
		
		
		
	</view>

</theme>
