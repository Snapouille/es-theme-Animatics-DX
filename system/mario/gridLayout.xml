<!-- Mario collection -->

<theme>
	<formatVersion>7</formatVersion>
	
	<view name="system">

		<image name="avatar" extra="static">
			<origin>0.5 0.5</origin>	
			<size>0.4510417 0.7925926</size>									
			<pos>0.83 1.03</pos>	
			<scale>0.1</scale>
			<linearSmooth>true</linearSmooth>		
			<rotation>10</rotation>
			
			<storyboard>
				
        		<animation property="offsetY" to="-0.50" begin="" duration="800" mode="Bump"/>
