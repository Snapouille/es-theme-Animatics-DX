<theme>
	<formatVersion>7</formatVersion>

	<view name="system">
	
		<image name="avatar" extra="true">
			<origin>0.5 0.5</origin>									
			<size>0.31 0.21</size>									
			<pos>0.57 0.63</pos>									
			<rotation>10</rotation>		
			<scale>0.4</scale>
			<zIndex>6</zIndex>
			
			<storyboard>
				<animation property="scale" to="1" begin="" duration="700" mode="EaseOutCubic"/>
				<animation property="x" to="0.82" begin="" duration="700" mode="EaseOutCubic"/>
				<animation property="y" to="0.78" begin="" duration="700" mode="EaseOutCubic"/>
				<animation property="rotation" from="10" to="0" begin="" duration="700" mode="EaseOutCubic"/>

			</storyboard>
			
			
		</image>

	</view>
	
</theme>
