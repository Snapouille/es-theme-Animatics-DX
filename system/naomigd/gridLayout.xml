<theme>
	<formatVersion>7</formatVersion>
	
	<view name="system">
	
	
		<image name="BG_snapLegacy" extra="true">
			<size>1 1</size>									
			<pos>0.5 0.44</pos>	
			<scale>1</scale>	
			<pos ifHelpPrompts="false">0.5 0.5</pos>	
			<saturation>0.7</saturation>
			  <storyboard>
				<animation property="saturation" to="1" begin="" duration="500" mode="linear"/>
			  </storyboard>
		</image>
		
	
		<image name="avatar" extra="true">
			<origin>0.5 0.5</origin>
			<size>0.1630208 0.6046296</size>									
			<pos>1.07 0.57</pos>									
			<path>./avatar.png</path>
			<opacity>1</opacity>
			<scale>1.15</scale>	

			<storyboard>
				<animation property="offsetX" to="-0.225" begin="100" duration="500" mode="EaseOut"/>
			</storyboard>
			
			<storyboard ifHelpPrompts="false">
				<animation property="offsetY"  to="0.06"/>
				<animation property="offsetX" to="-0.225" begin="100" duration="500" mode="EaseOut"/>
			</storyboard>
		</image>
		
		
		
	</view>
</theme>
