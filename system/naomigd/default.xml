<theme>
	<formatVersion>7</formatVersion>

	<view name="system">
		<image name="avatar" extra="static">
			<origin>0 0</origin>									
			<size>0.1630208 0.6046296</size>									
			<pos>0.2 0.15</pos>
			<scale>0.9</scale>
			<opacity>0</opacity>
		
			<storyboard>
					<animation property="opacity" from="0" to="1" begin="" duration="100" mode="linear"/>
					<animation property="x" to="0.27" begin="100" duration="3700" mode="EaseOut"/>
					<animation property="scale" to="1.2" begin="4000" duration="350" mode="bump"/>
					<animation property="x"  to="0.46" begin="4000" duration="375" mode="EaseOut"/>
					<animation property="y"  to="0.12" begin="4000" duration="375" mode="EaseOut"/>		
			</storyboard>
		</image>
		
		
	</view>

</theme>
