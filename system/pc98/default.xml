<theme>
	<formatVersion>7</formatVersion>

	<view name="system">
	
			<image name="avatar" extra="static">
			<origin>0 0</origin>
			<pos>0.3 0</pos>
			<maxSize>1 1</maxSize>
			<path>./avatar.png</path>
			<linearSmooth>true</linearSmooth>
			
			<storyboard>
				
        		<animation property="x" to="0.33" begin="" duration="4000" mode="EaseInOut"/>
        		
			</storyboard>	
		</image>

		
		
	</view>
	
</theme>
