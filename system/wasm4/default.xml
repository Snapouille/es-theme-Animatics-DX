<!-- Game Gear -->

<theme>
	<formatVersion>7</formatVersion>

	<view name="system">
		<image name="moon" extra="static">
			<origin>0 0</origin>									
			<pos>0.11 0.14</pos>
			<size>0.18125 0.3222222</size>									
			<scale>0.35</scale>
			<path>./moon.png</path>
			<opacity>1</opacity>
			<zIndex>14</zIndex>
			<linearSmooth>true</linearSmooth>
		</image>	
	
		
		<image name="sorciere" extra="static">
			<origin>0 0</origin>
			<pos>1 0.13</pos>
			<size>0.084375 0.1574074</size>									
			<scale>0.55</scale>
			<rotation>-10</rotation>
			<flipX>true</flipX>
			<path>./sorciere.png</path>
			<opacity>1</opacity>
			<zIndex>1800</zIndex>
			<linearSmooth>true</linearSmooth>
		</image>	
		
		<image name="sorciere2" extra="static">
			<origin>0 0</origin>
			<pos>-1 0.13</pos>
			<size>0.084375 0.1574074</size>									
			<scale>1</scale>
			
			<path>./sorciere.png</path>
			<opacity>1</opacity>
			<zIndex>14</zIndex>
			<linearSmooth>true</linearSmooth>
		</image>	
		
		
		
		
		
	<image name="moon" extra="true">	
		<storyboard repeat="forever">
		
				<animation property="x" to="0.58" begin="" autoreverse="true" duration="90000" mode="linear"/>
				<animation property="color" from="FFFFFF" to="202020" begin="30000" autoreverse="true" duration="60000" mode="EaseIn"/>
				
		</storyboard>
	</image>
	
	
	
	<image name="sorciere" extra="true">	
		<storyboard repeat="forever">
				
				<animation property="x" from="1" to="-1" begin="11500" duration="5500" mode="linear"/>
				<animation property="y" to="0.2" begin="13500" duration="3500" mode="bump"/>
				<animation property="opacity" to="0" begin="20000" duration="100" mode="linear"/>
				
		</storyboard>
	</image>
	
	
	<image name="sorciere2" extra="true">	
		<storyboard repeat="forever">
				
				<animation property="rotation" to="15" begin="" duration="10" mode="linear"/>
				
				<animation property="x" from="-0.5" to="0.36" begin="" duration="2500" mode="linear"/>
				<animation property="y" from="0.05" to="0.25" begin="" duration="2500" mode="bump"/>
				
				<animation property="x" from="0.36" to="0.362" begin="2500" autoreverse="true" duration="500" repeat="4" mode="linear"/>
				<animation property="rotation" from="15" to="16" begin="2500" autoreverse="true" duration="1000" repeat="2" mode="linear"/>
				<animation property="y" from="0.25" to="0.26" begin="2500" autoreverse="true" duration="500" repeat="4" mode="linear"/>
				<animation property="zIndex" to="2000" begin="2500" duration="10" mode="linear"/>
				
				<animation property="x" to="1.1" begin="6500" duration="2500" mode="linear"/>
				<animation property="y" to="0.13" begin="6500" duration="2500" mode="linear"/>
				
				<animation property="opacity" to="0" begin="20000" duration="100" mode="linear"/>
				
		</storyboard>
	</image>
	
	
	
	
	</view>
</theme>
