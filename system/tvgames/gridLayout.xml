<!-- TV Games -->

<theme>
	<formatVersion>7</formatVersion>
	
	<view name="system">

		<image name="avatar" extra="static">
			<origin>0.5 0.5</origin>	
			<size>0.29375 0.8398148</size>									
			<pos>0.8 1.5</pos>	
			<scale>0.2</scale>
			<linearSmooth>true</linearSmooth>					
			<storyboard>
				
        		<animation property="x" to="0.84" begin="" duration="500" mode="EaseInOut"/>
				<animation property="scale" to="1" begin="" duration="500" mode="Bump"/>
        		<animation property="y" to="0.6" begin="" duration="500" mode="Bump"/>
				
			</storyboard>					

		</image>
		
	</view>
</theme>
