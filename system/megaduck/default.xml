<theme>
	<formatVersion>7</formatVersion>

	<view name="system">
		
		
		<image name="avatar" extra="static">
			<origin>0 0</origin>									
			<size>0.3994792 0.762963</size>									

			<pos>0.1 0.167</pos>
			<scale>0.4</scale>
			<opacity>1</opacity>
			<storyboard>
				<animation property="x" to="0.2" begin="" duration="2200" mode="EaseOut"/>
				
				<animation property="scale" to="0.7" begin="2700" duration="350" mode="Linear"/>
				<animation property="x" to="0.38" begin="2700" duration="375" mode="EaseOut"/>
				<animation property="y" to="0.31" begin="2700" duration="375" mode="bump"/>
				
				
			</storyboard>
		</image>
		
		
	</view>

</theme>
