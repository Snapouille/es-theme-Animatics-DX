<theme>
 
  
  
  <view name="basic, detailed">
  
	
	<image name="ecran_console" extra="static">
			<visible>false</visible>
			
	</image>
	
	
  
  
	<image name="md_thumbnail">
		<origin>0.40 0.50</origin>
		<pos>0.398 0.34</pos> 
		<maxSize>0.233 0.597</maxSize>
		<!-- <size>0.242 0.61</size> -->
		<scale>0.55</scale>
		<color>FFFFFF</color>
		<zIndex>100</zIndex>
		
		
		<roundCorners>0.01</roundCorners>
		
		
		<storyboard event="open">
                <animation property="x" to="0.443" begin="1000" duration="200" mode="linear"/>
				<animation property="y" to="0.33" begin="1000" duration="200" mode="linear"/>
				<animation property="scale" to="1" begin="1000" duration="200" mode="linear"/>
		</storyboard>
			
		<storyboard event="activate">
                <animation property="x" to="0.443" begin="1000" duration="200" mode="linear"/>
				<animation property="y" to="0.33" begin="1000" duration="200" mode="linear"/>
				<animation property="scale" to="1" begin="1000" duration="200" mode="linear"/>
		</storyboard>
			
		<storyboard event="deactivate">
			    <animation property="opacity" to="0" duration="1" mode="linear"/>
		</storyboard>
		
		
	</image>
	
	
	
	
	<video name="md_video">
		  <pos>0.707 0.36</pos>
		  <!-- <size>0.225 0.31</size> -->
		  <maxSize>0.34 0.36</maxSize>
		  
				<storyboard event="open">
					<animation property="x" to="0.77" begin="1000" duration="200" mode="linear"/>
				</storyboard>
					
				<storyboard event="activate">
						<animation property="x" to="0.77" begin="1000" duration="200" mode="linear"/>
				</storyboard>
					
				<storyboard event="deactivate">
						<animation property="opacity" to="0" duration="1" mode="linear"/>
				</storyboard>
		  
		</video>
	
	
		
   </view>
  
  
  
</theme>
