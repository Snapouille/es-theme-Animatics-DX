			<pos>0.071 -0.487</pos>
			<color>FFFFFF00</color>
			<opacity>0</opacity>
			<zIndex>10</zIndex>
			
			<storyboard>
				<animation property="offsetY" to="1" begin="100" duration="100" mode="linear"/>

				<animation property="opacity" from="0" to="1" begin="310" duration="10" mode="Linear"/>
