<theme>
  
  
  <view name="system" ifSubset="SystemView:vertical|wheel">
		<image name="BottomBar" extra="true">
		  <visible>false</visible>
		</image>
		
		<image name="BottomBar-shadow" extra="true">
		  <visible>false</visible>
		</image>
				
